@model IEnumerable<EmployeeView>

<table border="1">
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.SurName</td>
            <td>@item.Patronymic</td>
            <td>@item.Age</td>
            <td>@item.Birthday</td>
            <td>@item.Sex</td>
            <td>@item.FamilyStatus</td>
            <td>@item.Position</td>

            <td>
                <a asp-action="Index">Спиоск</a>
           @*     @Html.ActionLink("Показать список", "Index")*@ 
            </td>
        </tr>
    }
</table>

