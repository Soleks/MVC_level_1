@model IEnumerable<WebStore.Models.EmployeeView>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" charset="utf-8" />
    <title>Index</title>
</head>
<body>
     Список

    <table border="1">
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.SurName</td>
            <td>@item.Patronymic</td>
            <td>@item.Age</td>
            <td>@Html.ActionLink("Показать детали", "Details", new { id = item.Id }) </td>
        </tr>
        }
    </table>
</body>
</html>
